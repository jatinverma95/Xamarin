<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HelloWorld.Exercises.InstagramApp.MyProfilePage"
             Title="Jatin Verma"
             xmlns:ic="clr-namespace:ImageCircle.Forms.Plugin.Abstractions;assembly=ImageCircle.Forms.Plugin.Abstractions">
    <StackLayout HorizontalOptions="Start" VerticalOptions="Start" Margin="0,20,0,0">
        <Label Text="Jatin Verma"/>
        <ic:CircleImage
                 WidthRequest="100"
                HeightRequest="100"
                Aspect="AspectFill"
                Source="http://via.placeholder.com/150x150"
                HorizontalOptions="Center"
                VerticalOptions="Center"/>
        <Label Text="My Name is Jatin Verma and I am Awesome"/>
    </StackLayout>
</ContentPage>